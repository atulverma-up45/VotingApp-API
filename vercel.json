{
    "version": 2,
    "functions": {
      "api": {
        "src": "/src/index.js",
        "runtime": "@now/node"
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/src/index.js"
      }
    ]
  }
  