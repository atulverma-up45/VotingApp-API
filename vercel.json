{
    "version": 2,
    "functions": {
      "api": {
        "handler": "src/index.handler",
        "runtime": "@vercel/node"
      }
    },
    "routes": [
      {
        "src": "/api/v1/(.*)",
        "dest": "src/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      }
    ]
  }
  